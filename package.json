{
  "name": "agio-docs",
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production vitepress build docs",
    "dev": "vitepress dev docs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "generate:api-docs": "tsx scripts/generate-api-docs.ts",
    "generate:spectaql": "tsx scripts/generate-spectaql.ts",
    "export:introspection": "tsx scripts/export-introspection.ts"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.0",
    "@vue/theme": "^2.3.0",
    "agio-sdk": "^1.1.7",
    "dotenv": "^17.2.3",
    "graphql": "^16.10.0",
    "highlight.js": "^11.11.1",
    "husky": "^4.3.8",
    "lint-staged": "^15.4.3",
    "markdown-it": "^14.1.0",
    "prettier": "^3.4.2",
    "spectaql": "^3.0.6",
    "terser": "^5.44.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.3",
    "vitepress": "^1.6.4",
    "vitest": "^3.0.4"
  },
  "engines": {
    "yarn": "4.x",
    "node": "24.x"
  },
  "packageManager": "yarn@4.6.0",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json,yml,yaml,css,html}": "prettier --write"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^2.5.0",
    "axios": "^1.7.9"
  }
}
